 <!--Ajax-->
<script>	 
	 function getCode(){
    
var phn_code= document.getElementById("phn_code2").innerHTML;
var phn_number= document.getElementById("phn_number").value;	
 document.getElementById("phnh").value=phn_number;
if(phn_number==''){document.getElementById("ph2").innerHTML='Please Enter Phone Number';}
else	{		$.ajax({
				type: "POST",
				url: "online_getCode.php",
				data: {phn_code:phn_code,phn_number:phn_number},
				dataType: "JSON",
				success: function(data) {
					$("#ph2").html(data);
				},
				error: function(err) {
					alert(err);
				}
			});   }
	 } 	 
 </script>	 
 
 
 
 <script>	 
 
 function getnum(){
    
	var num1= document.getElementById("num1").innerHTML;
	var num2= document.getElementById("num2").value;	

	if(num1!=''){num2=num1;}
	if(num2!=''){num1=num2;}

	var num3=num1+num2;

		if(num3!=''){
			document.getElementById("result").innerHTML=num3;
			}

} 	 
 </script>	
 
 
 
 
 
 
