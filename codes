        function send_sms($phone, $sms) {
		 	$to = $phone;
			$token = "a73835c6a4c2ba37ed58666d97e84496";
			$message =$sms;
			$url = "http://api.greenweb.com.bd/api.php";
			$data= array('to'=>"$to",'message'=>"$message",'token'=>"$token");  $ch = curl_init();  curl_setopt($ch, CURLOPT_URL,$url);curl_setopt($ch, CURLOPT_ENCODING, '');curl_setopt($ch, CURLOPT_POSTFIELDS, http_build_query($data));curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);curl_setopt($ch, CURLOPT_FORBID_REUSE, 1);curl_setopt($ch, CURLOPT_HTTPHEADER, array('Connection: Close'));$smsresult = curl_exec($ch);
         }
		 
        function send_email($subject, $sms, $contact) {
 			require_once('../login/mailer/class.phpmailer.php');

 			require_once('../login/mailer/class.phpmailer.php');
		    $mail = new PHPMailer(); 
			$mail->IsSMTP(); 
			$mail->SMTPDebug  = 0;                     
			$mail->SMTPAuth   = true;                  
			$mail->SMTPSecure = "ssl";                 
			$mail->Host       = "mail.softwarelagbe.com";        
			$mail->Port       = 465;             
			$mail->AddAddress($contact);
			$mail->Username="support@softwarelagbe.com";  
			$mail->Password="135host81451881s";            
			$mail->SetFrom('support@softwarelagbe.com','SoftwareLagbe IT SOLUTIONS');
			$mail->AddReplyTo("support@softwarelagbe.com","SoftwareLagbe IT SOLUTIONS");
			$mail->Subject    = $subject;
			$mail->MsgHTML($sms);
			$mail->Send();        
		}		 
			
		function today() {
			$d1 = new DateTime('now', new DateTimezone('Asia/Dhaka')); $d1=$d1->format('F j, Y, g:i a');
			return $d1;
		}
		
		function today() {		
$timeFirst  = strtotime('2011-05-13 18:20:10');
$timeSecond = strtotime('2011-05-13 18:20:20');
$differenceInSeconds = $timeSecond - $timeFirst;
return $differenceInSeconds;	
		}
		
		function phone($phone) { 
			$phone = str_replace(' ', '', $phone); if (strpos($phone, '+') !== false) { $phone=str_replace('+', '', $phone); } if (substr($phone, 0, 2 ) === "88") { $phone=str_replace('88', '', $phone); }
			return $phone;
		}		
		
		
